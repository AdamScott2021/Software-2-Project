<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="417.0" prefWidth="393.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ModCustomer">
    <children>
        <Button fx:id="savePt" layoutX="23.0" layoutY="375.0" mnemonicParsing="false" onAction="#saveCust" text="Save" />
        <Button fx:id="canPt" layoutX="72.0" layoutY="375.0" mnemonicParsing="false" onAction="#canCust" text="Cancel" />
        <Label layoutX="22.0" layoutY="21.0" text="View/Modify Customer">
            <font>
                <Font name="Courier New" size="24.0" />
            </font>
        </Label>
        <Label layoutX="22.0" layoutY="203.0" text="Address" />
        <Label layoutX="25.0" layoutY="260.0" text="Zip Code" />
        <Label layoutX="220.0" layoutY="75.0" text="Phone Number" />
        <Label layoutX="22.0" layoutY="134.0" text="Full Name" />
        <Label layoutX="23.0" layoutY="75.0" text="Customer ID" />
        <Label layoutX="218.0" layoutY="203.0" text="Division" />
        <Label layoutX="217.0" layoutY="134.0" text="Country" />
        <TextField fx:id="modPhone" layoutX="217.0" layoutY="97.0" />
        <TextField fx:id="modAddress" layoutX="23.0" layoutY="220.0" />
        <TextField fx:id="modZip" layoutX="23.0" layoutY="285.0" />
        <TextField fx:id="modName" layoutX="23.0" layoutY="155.0" />
        <TextField fx:id="modID" editable="false" layoutX="23.0" layoutY="97.0" />
        <ComboBox fx:id="divisionBox" layoutX="216.0" layoutY="220.0" prefWidth="150.0" />
        <ComboBox fx:id="countryBox" layoutX="216.0" layoutY="155.0" onAction="#countryBoxAction" prefWidth="150.0" />
    </children>
</AnchorPane>
